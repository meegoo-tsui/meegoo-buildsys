include buildsys.mak

#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
all: config_env
	set -e; \
	cd $(SRC_DIR)/RTL8676/RTL8676xx; \
	make prepare; \
	make

#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
clean:
	rm -f buildsys.mak

#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
install:
	echo "make install"

#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
config_env:
	./config.sh

