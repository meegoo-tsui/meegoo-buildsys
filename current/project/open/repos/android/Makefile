include buildsys.mak

#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
all:
	@echo "make all ..."; \
	export SRC_DIR=$(SRC_DIR); \
	./repo.sh

#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
clean: 
	@echo "make clean ..."
	rm -f buildsys.mak

#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
install: 
	@echo "make install ..."
	@echo "空操作，无任何动作执行!"

